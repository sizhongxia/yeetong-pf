<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:motan="http://api.weibo.com/schema/motan"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
   http://api.weibo.com/schema/motan http://api.weibo.com/schema/motan.xsd">

	<motan:registry regProtocol="zookeeper" name="registry" address="192.168.1.105:2181" />

	<bean id="userServiceImpl" class="org.yeetong.rpc.service.srpcs.UserServiceImple"></bean>

	<motan:service interface="org.yeetong.rpc.interfaces.UserService" ref="userServiceImpl" group="yeetong" module="db" version="1.0" export="8002" />

</beans>